{
  "name": "atlas-exports-server",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx watch index.ts",
    "build": "esbuild index.ts --platform=node --bundle --format=cjs --outfile=dist/index.cjs --minify --external:firebase-admin",
    "start": "cross-env NODE_ENV=production node dist/index.cjs",
    "check": "tsc --noEmit"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "firebase-admin": "^13.6.0",
    "zod": "^3.24.2",
    "zod-validation-error": "^3.4.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "4.17.21",
    "@types/node": "20.16.11",
    "cross-env": "^7.0.3",
    "esbuild": "^0.25.0",
    "tsx": "^4.20.5",
    "typescript": "5.6.3"
  }
}
